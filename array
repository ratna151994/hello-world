import java.util.*;
class Array
{
	public static void main(String[] args)
	{
		int arr[]=new int[5];
		int b1[]={1,2,3,4,5};
		arr[0]=1;
		Scanner s=new Scanner(System.in);
		System.out.println("Enter 4 numbers");
		for(int i=1;i<5;i++)
		{
			arr[i]=s.nextInt();
		}
		for(int i=0;i<5;i++)
		{
			System.out.println(arr[i]);
		}
		for(int i=0;i<5;i++)
		{
			System.out.println(b1[i]);
		}
	}
}
